<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.storing3.MainActivity">
    <EditText
        android:id="@+id/lbl_titleId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <EditText
        android:id="@+id/lbl_subtitleId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/lbl_savedBtn"
        android:text="@string/lbl_save_txtBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/lbl_readBtn"
        android:text="@string/lbl_read_txtBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/lbl_result_Tv_Id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
         />

</LinearLayout>
